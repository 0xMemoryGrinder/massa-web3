{
    "name": "@massalabs/massa-web3",
    "version": "1.16.1",
    "description": "massa's web3 sdk client",
    "main": "./lib.commonjs/index.js",
    "module": "./lib.esm/index.js",
    "scripts": {
        "up": "npm update",
        "check": "npm outdated",
        "upgrade": "npm run up && npm run check && npm run build",
        "build-commonjs": "tsc --project tsconfig.commonjs.json",
        "build-esm": "tsc --project tsconfig.esm.json",
        "build-rollup": "rollup -c",
        "uglify-all": "uglifyjs ./dist/massa.js -o ./dist/massa.min.js && uglifyjs ./dist/massa.umd.js -o ./dist/massa.umd.min.js",
        "build": "rimraf dist/* && npm run build-commonjs && npm run build-esm && npm run build-rollup && npm run uglify-all",
        "update-version-major": "npm version major",
        "update-version-minor": "npm version minor",
        "update-version-patch": "npm version patch",
        "test": "jest",
        "test-smart-contract-example": "ts-node ./examples/smartContracts/index.ts",
        "test-wallet": "ts-node ./examples/wallet/index.ts",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "fmt": "npm run prettier:fix && npm run lint:fix",
        "fmt:check": "npm run prettier && npm run lint",
        "doc": "typedoc src/index.ts --name massa-web3 --out docs/documentation/html --tsconfig tsconfig.json",
        "test:web3-browser": "python3 -m http.server 9007"
    },
    "author": "Massa Labs <info@massa.net>",
    "contributors": [
        "Evgeni Pirianov",
        "Yvan Sraka <yvan@sraka.xyz>"
    ],
    "license": "(MIT AND Apache-2.0)",
    "homepage": "https://github.com/massalabs/massa-web3",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/massalabs/massa-web3"
    },
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "keywords": [
        "web3",
        "ts",
        "sdk",
        "massa"
    ],
    "files": [
        "dist",
        "bundle.js",
        "bundle.min.js"
    ],
    "dependencies": {
        "@noble/ed25519": "^1.7.3",
        "@noble/hashes": "^1.2.0",
        "@rollup/plugin-commonjs": "^24.1.0",
        "bignumber.js": "^9.1.1",
        "bs58": "^5.0.0",
        "buffer": "^6.0.3",
        "cross-fetch": "^3.1.5",
        "crypto-js": "^4.1.1",
        "dotenv": "^16.0.3",
        "esmify": "^2.1.1",
        "http-server": "^14.1.1",
        "jest-environment-jsdom": "^29.5.0",
        "js-base64": "^3.7.5",
        "rollup-plugin-json": "4.0.0",
        "rollup-plugin-node-polyfills": "^0.2.1"
    },
    "devDependencies": {
        "@massalabs/eslint-config": "^0.0.9",
        "@massalabs/prettier-config-as": "^0.0.2",
        "@rollup/plugin-node-resolve": "15.0.2",
        "@types/bn.js": "^5.1.1",
        "@types/bs58": "^4.0.1",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.13.0",
        "@types/secp256k1": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "@typescript-eslint/parser": "^5.52.0",
        "babel-jest": "^29.5.0",
        "browserify": "^17.0.0",
        "chalk": "^4.1.0",
        "eslint": "^8.34.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.25.5",
        "eslint-plugin-jsdoc": "^40.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jsdom": "^21.1.1",
        "lint-staged": "^13.1.2",
        "node-ts": "^5.1.2",
        "ora": "^5.1.0",
        "prettier": "^2.8.4",
        "prettier-eslint": "^15.0.1",
        "prettier-standard": "^16.4.1",
        "rollup": "3.21.2",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "tslint": "^6.1.3",
        "typedoc": "^0.23.25",
        "typescript": "^4.9.5",
        "uglify-js": "^3.17.4",
        "uglifyify": "^5.0.2"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.7",
        "utf-8-validate": "^6.0.2"
    },
    "prettier": "@massalabs/prettier-config-as",
    "engines": {
        "node": ">=14.0.0"
    }
}
